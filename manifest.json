{
  "manifest_version": 2,
  "name": "gimana2",
  "description": "This extension transcribes audio to  text",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "gimana2"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/transcription.js"]
    }
  ],
  "permissions": ["activeTab"]
}
